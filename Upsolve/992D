	{
		cin >> arr[i];
	}	
	int j = 1;
	while(j<=n)
	{
		if(arr[j]==1)
		{
			int cnt=0;
			while(j<=n && arr[j]==1)
			{	
				cnt++;
				j++;
			}
			nums.pb(mp(1, cnt));
		}
		else
		{
			nums.pb(mp(arr[j], 1));
			j++;
		}
	}
	for(int i = 1; i <= n; i++)
	{
		
		for(int j = i; j >= 1; j--)
		{

		}
	}
}